@model Slider
@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/AdminF/Views/Shared/_AdminLayout.cshtml";
}

<div class="container">
    <div class="row">
        <form method="post" enctype="multipart/form-data" >
            <div asp-validation-summary=ModelOnly> </div>
            <div class="mb-3">
                <input asp-for="Photo"  class="form-control" accept="iamge/*">
                <label asp-for="Photo" class="form-label">Photo</label>
            </div>
            <div class="mb-3">
                <label asp-for="PhotoLogo" class="form-label" accept="iamge/*">PhotoLogo</label>
                <input asp-for="PhotoLogo" class="form-control"  >
            </div>
            <button type="submit" class="btn btn-primary">Create</button>
        </form>
    </div>
</div>